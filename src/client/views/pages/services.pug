extends ../templates/base.pug
block meta
    meta(charset='UTF-8')
    meta(
        name='keywords',
        content='Foster Z Portfolio Works'
    )
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='author', content='Foster Z')
    meta(
        name='description',
        content='Foster Z | Portfolio | Services '
    )
    meta(name='robots', content='index,nofollow')
    meta(http-equiv='X-UA-Compatible', content='ie=edge')
    link(rel='canonical', href='https://fosterz.netlify.app')
block title
    | Foster Z | Portfolio | Services
block content
    - var webservices = ['Modern Website Design', 'API Design', 'Asthetic Front End Web Application', 'Secure Back End Development', 'Custom Website Design Of Any Scale', 'Website Using Modern Frameworks Like React, Angular, Vue & More']
    .services-container
        .services
            include ../components/service.pug
            +service-card("1", "Web Development", webservices, true)
            +service-card("2", "Mobile Development", [])
            +service-card("3", "Desktop Development", [])
            +service-card("4", "BlockChain Development", [])
